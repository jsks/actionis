{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true
        }
    },
    "env": {
        "node": true
    },
    "rules": {
        "no-console": 0,
        "consistent-return": 0,
        "curly": [1, "multi-or-nest"],
        "dot-location": [1, "property"],
        "dot-notation": 1,
        "guard-for-in": 1,
        "no-alert": 1,
        "no-caller": 1,
        "no-empty-pattern": 1,
        "no-eq-null": 1,
        "no-eval": 1,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-labels": 1,
        "no-implicit-coercion": 1,
        "no-implicit-globals": 1,
        "no-implied-eval": 1,
        "no-invalid-this": 1,
        "no-iterator": 1,
        "no-lone-blocks": 1,
        "no-loop-func": 1,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-native-reassign": 1,
        "no-new": 1,
        "no-new-wrappers": 1,
        "no-octal-escape": 1,
        "no-param-reassign": 1,
        "no-proto": 1,
        "no-self-compare": 1,
        "no-sequences": 1,
        "no-unused-expressions": 1,
        "no-useless-call": 1,
        "no-useless-concat": 1,
        "no-useless-escape": 1,
        "no-void": 1,
        "no-with": 1,
        "radix": [1, "as-needed"],
        "strict": [0, "safe"],
        "init-declarations": [1, "always"],
        "no-catch-shadow": 1,
        "no-use-before-define": 0,
        "handle-callback-err": 1,
        "no-new-require": 1,
        "array-bracket-spacing": [1, "never"],
        "block-spacing": [1, "always"],
        "brace-style": [1, "1tbs", { "allowSingleLine": true }],
        "camelcase": [1, { "properties": "always" }],
        "comma-spacing": 1,
        "comma-style": 1,
        "computed-property-spacing": 1,
        "indent": [0, 4, {  "VariableDeclarator": { "const": 1 }}],
        "key-spacing": 1,
        "keyword-spacing": 1,
        "lines-around-comment": 1,
        "max-len": [1, 100],
        "new-parens": 1,
        "newline-after-var": 1,
        "newline-before-return": 1,
        "newline-per-chained-call": [1, { "ignoreChainWithDepth": 3 }],
        "no-lonely-if": 1,
        "no-multiple-empty-lines": 1,
        "no-nested-ternary": 1,
        "no-plusplus": 1,
        "no-spaced-func": 1,
        "no-underscore-dangle": 1,
        "no-unneeded-ternary": 1,
        "no-whitespace-before-property": 1,
        "object-curly-spacing": [1, "always"],
        "operator-assignment": [1, "always"],
        "operator-linebreak": [1, "before"],
        "padded-blocks": [1, "never"],
        "quote-props": [1, "as-needed"],
        "quotes": [1, "single"],
        "semi": [1, "never"],
        "space-before-function-paren": [1, "never"],
        "space-in-parens": [1, "never"],
        "space-infix-ops": 1,
        "space-unary-ops": 1,
        "spaced-comment": [1, "always"],
        "arrow-spacing": 1,
        "generator-star-spacing": [1, {"before": false, "after": true}],
        "no-const-assign": 1,
        "no-var": 1,
        "object-shorthand": 1,
        "prefer-arrow-callback": 1,
        "prefer-const": 1,
        "prefer-rest-params": 1,
        "prefer-spread": 1,
        "require-yield": 1,
        "template-curly-spacing": [1, "never"],
        "yield-star-spacing": 1
    }
}
